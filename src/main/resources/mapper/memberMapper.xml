<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="global.sesoc.tempcat.dao.MemberMapper">
	<select id="checkId" parameterType="Member">
		select *
		from tempcat_member
		where id = #{id}
	</select>


